                                    "        <div><div class=\"label\">Unit</div><div class=\"val\">{{response.unit.name}}</div></div>",
                                    "        <div><div class=\"label\">Quantity</div><div class=\"val\">{{response.quantity.name}}</div></div>",
                                    "        <div><div class=\"label\">System ID</div><div class=\"val\">{{response.id}}</div></div>",
                                    "    </div>",
                                    "    ",
                                    "    <div class=\"section-title\">Historical Recommended Values</div>",
                                    "    <table>",
                                    "        <thead><tr><th>Year</th><th>Value</th><th>Uncertainty</th></tr></thead>",
                                    "        <tbody>",
                                    "            {{#each response.historicalValues}}",
                                    "            <tr>",
                                    "                <td class=\"v-id\">{{versionId}}</td>",
                                    "                <td><code>{{value}}</code> {{#if isExact}}<span style=\"color: #38a169; font-weight: bold; font-size: 0.8em;\">(Exact)</span>{{/if}}</td>",
                                    "                <td>{{#if uncertainty}}{{uncertainty}}{{else}}-{{/if}}</td>",
                                    "            </tr>",
                                    "        {{/each}}",
                                    "    </tbody>",
                                    "</table>",
                                    "</div>",
                                    "`;",
                                    "pm.visualizer.set(template, { response: pm.response.json() });"
                                ],
                                "type": "text/javascript"
                            }
